"UnLitTwoTexture"
{
	"$baseTexture" "vcmod/lights/hbeam"
	"$texture2" "vcmod/lights/hbeam_scroll"
	"$nocull" 1
	"$additive" 1
	"$translucent" 1
	"$vertexalpha" 1
	"$vertexcolor" 1
	"Proxies"
	{
		"Equals"
		{
			"$srcVar1" "$color"
			"$resultVar" "$color2"
		}

		"TextureScroll"
		{
			"texturescrollvar" "$texture2transform"
			"texturescrollrate" 0.02
			"texturescrollangle" -135
		}

	}
}
